def linear_search(arr,target):
    found=False
    for i in range(len(arr)):
        if arr[i]==target:
            print("element found at position:",i)
            found=true
            break
        if not found:
            print("element not found")
            arr=[10,20,30,40,50]
            target=int(input("enter element to serach:"))
            linear_search(arr,target)
